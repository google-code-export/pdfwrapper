<?xml version="1.0"?>
<skin version="1.1" name="Bekle" author="LongTail Video">

	<components>
		<component name="controlbar">
			<settings>
				<setting name="margin" value="0" />
				<setting name="fontsize" value="10" />
				<setting name="fontcolor" value="0x00b9ff"/>
				<setting name="buttoncolor" value="0xffffff"/>
			</settings>

			<layout>
				<group position="left">
					<button name="play" />
				</group>
				<group position="center">
					<slider name="time" />
				</group>
				<group position="right">
					<button name="blank" />
					<slider name="volume" />
					<button name="fullscreen" />
				</group>
			</layout>

			<elements>
				<element name="background" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/cap.png" />
				<element name="blankButton" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/divider.png" />
				<element name="capLeft" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/cap.png" />
				<element name="capRight" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/cap.png" />
				<element name="divider" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/divider.png" />
				<element name="playButton" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/playButton.png" />
				<element name="playButtonOver" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/playButtonOver.png" />
				<element name="pauseButton" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/pauseButton.png" />
				<element name="pauseButtonOver" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/pauseButtonOver.png" />
				<element name="prevButton" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/prevButton.png" />
				<element name="prevButtonOver" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/prevButtonOver.png" />
				<element name="nextButton" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/nextButton.png" />
				<element name="nextButtonOver" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/nextButtonOver.png" />
				<element name="timeSliderRail" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/timeSliderRail.png" />
				<element name="timeSliderBuffer" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/timeSliderBuffer.png" />
				<element name="timeSliderProgress" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/timeSliderProgress.png" />
				<element name="timeSliderCapLeft" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/timeSliderCapLeft.png" />
				<element name="timeSliderCapRight" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/timeSliderCapRight.png" />
				<element name="volumeSliderRail" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/volumeSliderRail.png" />
				<element name="volumeSliderProgress" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/volumeSliderProgress.png" />
				<element name="volumeSliderCapLeft" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/divider.png" />
				<element name="volumeSliderCapRight" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/divider.png" />
				<element name="fullscreenButton" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/fullscreenButton.png" />
				<element name="fullscreenButtonOver" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/fullscreenButtonOver.png" />
				<element name="normalscreenButton" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/normalscreenButton.png" />
				<element name="normalscreenButtonOver" src="https://kandangantvonline.googlecode.com/svn/tags/controlbar/normalscreenButtonOver.png" />
			</elements>
		</component>
		
		<component name="display">
			<settings>
				<setting name="backgroundcolor" value="0x000000" />
				<setting name="bufferinterval" value="150" />
				<setting name="bufferrotation" value="90" />
			</settings>
			<elements>
				<element name="background" src="https://kandangantvonline.googlecode.com/svn/tags/display/background.png" />
				<element name="playIcon" src="https://kandangantvonline.googlecode.com/svn/tags/display/playIcon.png" />
				<element name="muteIcon" src="https://kandangantvonline.googlecode.com/svn/tags/display/muteIcon.png" />
				<element name="errorIcon" src="https://kandangantvonline.googlecode.com/svn/tags/display/bufferIcon.png" />
				<element name="bufferIcon" src="https://kandangantvonline.googlecode.com/svn/tags/display/bufferIcon.png" />
			</elements>
		</component>
		
		<component name="dock">
			<settings>
				<setting name="fontcolor" value="0xffffff" />
			</settings>
			<elements>
				<element name="button" src="https://kandangantvonline.googlecode.com/svn/tags/dock/button.png" />
			</elements>
		</component>
		
		<component name="playlist">
			<settings>
				<setting name="fontcolor" value="0xffffff" />
				<setting name="overcolor" value="0xffffff" />
				<setting name="activecolor" value="0x00adef" />
				<setting name="backgroundcolor" value="0x001319" />
				<setting name="thumbs" value="false" />
			</settings>
			<elements>
				<element name="item" src="https://kandangantvonline.googlecode.com/svn/tags/playlist/item.png" />
				<element name="itemOver" src="https://kandangantvonline.googlecode.com/svn/tags/playlist/itemOver.png" />
				<element name="sliderRail" src="https://kandangantvonline.googlecode.com/svn/tags/playlist/sliderRail.png" />
				<element name="sliderThumb" src="https://kandangantvonline.googlecode.com/svn/tags/playlist/sliderThumb.png" />
				<element name="sliderCapTop" src="https://kandangantvonline.googlecode.com/svn/tags/playlist/sliderCap.png" />
				<element name="sliderCapBottom" src="https://kandangantvonline.googlecode.com/svn/tags/playlist/sliderCap.png" />
			</elements>
		</component>
		
		<component name="sharing">
			<elements>
				<element name="embedIcon" src="https://kandangantvonline.googlecode.com/svn/tags/sharing/embedIcon.png" />
				<element name="embedScreen" src="https://kandangantvonline.googlecode.com/svn/tags/sharing/embedScreen.png" />
				<element name="shareIcon" src="https://kandangantvonline.googlecode.com/svn/tags/sharing/shareIcon.png" />
				<element name="shareScreen" src="https://kandangantvonline.googlecode.com/svn/tags/sharing/shareScreen.png" />
			</elements>
		</component>

	</components>

</skin>